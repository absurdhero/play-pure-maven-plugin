<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="Maven" xmlns="http://maven.apache.org/DECORATION/1.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">
    <bannerLeft>
        <name>Play Pure-Maven Plugin</name>
        <href>https://github.com/absurdhero/play-pure-maven-plugin</href>
    </bannerLeft>

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.0</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
        </fluidoSkin>
        <gitHub>
          <projectId>absurdhero/play-pure-maven-plugin</projectId>
          <ribbonOrientation>right</ribbonOrientation>
          <ribbonColor>black</ribbonColor>
        </gitHub>
    </custom>


    <body>
        <links>
            <item name="README" href="https://github.com/absurdhero/play-pure-maven-plugin/blob/master/README.rst"/>
            <item name="Nominum" href="http://www.nominum.com"/>
        </links>

        <menu ref="reports"/>
    </body>
</project>
